{
  "expo": {
    "name": "Stark",
    "slug": "stark-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "stark",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/logo/FullLogo.png",
          "dark": {
            "image": "./assets/images/logo/FullLogo.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 250
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/TT-Octosquares-Medium.ttf"
          ]
        }
      ],
      "expo-router"
    ],
    "android": {
      "package": "com.starkcabs.stark",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDZ3-iM9suiGVssgSjEdApgQXpXmDgvVAc"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "icon": "...",
      "bundleIdentifier": "com.starkcabs.stark",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDZ3-iM9suiGVssgSjEdApgQXpXmDgvVAc"
      }
    },
    "extra": {
      "eas": {
        "projectId": "3cf9105e-ca08-4cbc-964b-2040139bbcd9"
      },
      "router": {}
    },
    "owner": "stark-opc-pvt-ltd"
  }
}
